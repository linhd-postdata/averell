    <body>
      <head>
        <title>Aunque de Escitia fueras,</title>
      </head>
      <lg cert="high" type="Liras">
        <l met="-|-|-|4|-|6|-|" n="1">
          <!--Aunque de Escitia fueras,-->
          <w lemma="aunque" type="CC">aun|que|</w>
          <w lemma="de" type="SP">de</w>
          <w lemma="escitia" type="NP00000">es|ci|tia|</w>
          <w lemma="ser" type="VSSI2S0">fue|ras|</w>
        </l>
        <l met="-|-|3|4|-|6|-|-|-|10|-|" n="2">
          <!--y aunque más bravo fuera tu marido,-->
          <w lemma="y" type="CC">y</w>
          <w lemma="aunque" type="CC">aun|que|</w>
          <w lemma="más" type="RG">más|</w>
          <w lemma="bravo" type="AQ0MS00">bra|vo|</w>
          <w lemma="ser" type="VSSI3S0">fue|ra|</w>
          <w lemma="tu" type="DP2CSS">tu|</w>
          <w lemma="marido" type="NCMS000">ma|ri|do|</w>
        </l>
        <l met="-|-|3|-|-|6|-|" n="3">
          <!--condolerte debieras,-->
          <w lemma="condoler" type="VMN0000">con|do|ler|</w>
          <w lemma="te" type="PP2CS00">te|</w>
          <w lemma="deber" type="VMSI2S0">de|bie|ras|</w>
        </l>
        <l met="1|-|-|-|-|6|-|" n="4">
          <!--Lice, del que ofrecido-->
          <w lemma="lice" type="NP00000">li|ce|</w>
          <w lemma="del" type="SP">del|</w>
          <w lemma="que" type="PR0CN00">que</w>
          <w lemma="ofrecer" type="VMP00SM">o|fre|ci|do|</w>
        </l>
        <l met="-|2|-|4|-|-|-|8|-|10|-|" n="5">
          <!--al cierzo tienes en tu umbral tendido.-->
          <w lemma="al" type="SP">al|</w>
          <w lemma="cierzo" type="NCMS000">cier|zo|</w>
          <w lemma="tener" type="VMIP2S0">tie|nes|</w>
          <w lemma="en" type="SP">e|n</w>
          <w lemma="tu" type="DP2CSS">tu</w>
          <w lemma="umbral" type="NCMS000">um|bral|</w>
          <w lemma="tender" type="VMP00SM">ten|di|do|</w>
        </l>
        <l met="-|2|-|-|-|6|-|" n="6">
          <!--¿La puerta, la arboleda-->
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="puerta" type="NCFS000">puer|ta|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="arboleda" type="NCFS000">ar|bo|le|da|</w>
        </l>
        <l met="1|-|-|4|-|6|-|-|-|10|-|" n="7">
          <!--oyes del fiero viento combatida,-->
          <w lemma="oír" type="VMIP2S0">o|yes|</w>
          <w lemma="del" type="SP">del|</w>
          <w lemma="fiero" type="AQ0MS00">fie|ro|</w>
          <w lemma="viento" type="NCMS000">vien|to|</w>
          <w lemma="combatir" type="VMP00SF">com|ba|ti|da|</w>
        </l>
        <l met="1|2|-|4|-|6|-|" n="8">
          <!--cuál brama?, ¿cuál se queda-->
          <w lemma="cuál" type="PT0CS00">cuál|</w>
          <w lemma="cuál" type="PT0CS00">cuál|</w>
          <w lemma="bramar" type="VMIP3S0">bra|ma|</w>
          <w lemma="cuál" type="PT0CS00">cuál|</w>
          <w lemma="cuál" type="PT0CS00">cuál|</w>
          <w lemma="se" type="P00CN00">se|</w>
          <w lemma="quedar" type="VMIP3S0">que|da|</w>
        </l>
        <l met="-|2|-|4|-|6|-|" n="9">
          <!--la nieve ya caída-->
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="nieve" type="NCFS000">nie|ve|</w>
          <w lemma="ya" type="RG">ya|</w>
          <w lemma="caída" type="NCFS000">ca|í|da|</w>
        </l>
        <l met="-|2|-|4|-|6|-|-|-|10|-|" n="10">
          <!--del aire agudo en mármol convertida?-->
          <w lemma="del" type="SP">del|</w>
          <w lemma="aire" type="NCMS000">ai|re</w>
          <w lemma="agudo" type="AQ0MS00">a|gu|do</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="mármol" type="NCMS000">már|mol|</w>
          <w lemma="convertir" type="VMP00SF">con|ver|ti|da|</w>
        </l>
        <l met="1|-|3|-|-|6|-|" n="11">
          <!--Deja, que es desamada-->
          <w lemma="dejar" type="VMIP3S0">de|ja|</w>
          <w lemma="que" type="PR0CN00">que</w>
          <w lemma="ser" type="VSIP3S0">es|</w>
          <w lemma="desamar" type="VMP00SF">de|sa|ma|da|</w>
        </l>
        <l met="-|2|-|4|-|-|-|8|-|10|-|" n="12">
          <!--de Venus esa tu soberbia vana,-->
          <w lemma="de" type="SP">de|</w>
          <w lemma="venus" type="NP00000">ve|nus|</w>
          <w lemma="ese" type="DD0FS0">e|sa|</w>
          <w lemma="tu" type="DP2CSS">tu|</w>
          <w lemma="soberbia" type="NCFS000">so|ber|bia|</w>
          <w lemma="vano" type="AQ0FS00">va|na|</w>
        </l>
        <l met="1|-|3|-|-|6|-|" n="13">
          <!--no te halles burlada,-->
          <w lemma="no" type="RN">no|</w>
          <w lemma="te" type="PP2CS00">te|</w>
          <w lemma="hall" type="NCMP000">ha|lles|</w>
          <w lemma="burlar" type="VMP00SF">bu|rla|da|</w>
        </l>
        <l met="1|-|-|4|-|6|-|" n="14">
          <!--no te engendró toscana-->
          <w lemma="no" type="RN">no|</w>
          <w lemma="te" type="PP2CS00">te</w>
          <w lemma="engendrar" type="VMIS3S0">en|gen|dró|</w>
          <w lemma="toscano" type="AQ0FS00">tos|ca|na|</w>
        </l>
        <l met="-|2|-|-|-|6|-|-|-|10|-|" n="15">
          <!--a ser como Penélope inhumana.-->
          <w lemma="a" type="SP">a|</w>
          <w lemma="ser" type="VSN0000">ser|</w>
          <w lemma="como" type="CS">co|mo|</w>
          <w lemma="penélope" type="NP00000">pe|né|lo|pe</w>
          <w lemma="inhumano" type="AQ0FS00">i|nhu|ma|na|</w>
        </l>
        <l met="-|-|-|-|-|6|-|" n="16">
          <!--¡Oh!, aunque a domeñarte-->
          <w lemma="oh" type="I">oh</w>
          <w lemma="aunque" type="CC">aun|que|</w>
          <w lemma="a" type="SP">a</w>
          <w lemma="domeñar" type="VMN0000">do|me|ñar|</w>
          <w lemma="te" type="PP2CS00">te|</w>
        </l>
        <l met="-|-|-|4|-|6|-|8|-|10|-|" n="17">
          <!--ni tu marido de otro amor tocado,-->
          <w lemma="ni" type="CC">ni|</w>
          <w lemma="tu" type="DP2CSS">tu|</w>
          <w lemma="marido" type="NCMS000">ma|ri|do|</w>
          <w lemma="de" type="SP">de</w>
          <w lemma="otro" type="DI0MS0">o|tro</w>
          <w lemma="amor" type="NCMS000">a|mor|</w>
          <w lemma="tocar" type="VMP00SM">to|ca|do|</w>
        </l>
        <l met="-|2|-|4|5|6|-|" n="18">
          <!--ni ruego ni oro es parte,-->
          <w lemma="ni" type="CC">ni|</w>
          <w lemma="ruego" type="NCMS000">rue|go|</w>
          <w lemma="ni" type="CC">ni|</w>
          <w lemma="oro" type="NCMS000">o|ro</w>
          <w lemma="ser" type="VSIP3S0">es|</w>
          <w lemma="parte" type="NCCS000">par|te|</w>
        </l>
        <l met="-|-|-|-|-|6|-|" n="19">
          <!--ni del enamorado-->
          <w lemma="ni" type="CC">ni|</w>
          <w lemma="del" type="SP">del|</w>
          <w lemma="enamorado" type="NCMS000">e|na|mo|ra|do|</w>
        </l>
        <l met="-|-|-|4|-|6|-|-|-|10|-|" n="20">
          <!--la amarillez teñida de violado,-->
          <w lemma="el" type="DA0FS0">la</w>
          <w lemma="amarillez" type="NCFS000">a|ma|ri|llez|</w>
          <w lemma="teñir" type="VMP00SF">te|ñi|da|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="violar" type="VMP00SM">vio|la|do|</w>
        </l>
        <l met="1|2|-|-|-|6|-|" n="21">
          <!--un poco de mesura-->
          <w lemma="uno" type="DI0MS0">un|</w>
          <w lemma="poco" type="PI0MS00">po|co|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="mesura" type="NCFS000">me|su|ra|</w>
        </l>
        <l met="1|-|-|4|-|6|-|8|-|10|-|" n="22">
          <!--usa conmigo, ¡oh, sierpe, oh, más que yerta-->
          <w lemma="usar" type="VMIP3S0">u|sa|</w>
          <w lemma="conmigo" type="PP1CSO0">con|mi|go</w>
          <w lemma="oh" type="I">oh|</w>
          <w lemma="sierpe" type="NCFS000">sier|pe</w>
          <w lemma="oh" type="I">oh|</w>
          <w lemma="más" type="RG">más|</w>
          <w lemma="que" type="CS">que|</w>
          <w lemma="yerto" type="AQ0FS00">yer|ta|</w>
        </l>
        <l met="-|2|-|4|-|6|-|" n="23">
          <!--encina y roble dura!-->
          <w lemma="encina" type="NCFS000">en|ci|na</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="roble" type="NCMS000">ro|ble|</w>
          <w lemma="duro" type="AQ0FS00">du|ra|</w>
        </l>
        <l met="-|2|3|-|-|6|-|" n="24">
          <!--Que no siempre tu puerta-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="no" type="RN">no|</w>
          <w lemma="siempre" type="RG">siem|pre|</w>
          <w lemma="tu" type="DP2CSS">tu|</w>
          <w lemma="puerta" type="NCFS000">puer|ta|</w>
        </l>
        <l met="-|2|-|4|-|6|-|-|-|10|-|" n="25">
          <!--podré sufrir al agua descubierta.-->
          <w lemma="poder" type="VMIF1S0">po|dré|</w>
          <w lemma="sufrir" type="VMN0000">su|frir|</w>
          <w lemma="al" type="SP">al|</w>
          <w lemma="agua" type="NCCS000">a|gua|</w>
          <w lemma="descubrir" type="VMP00SF">des|cu|bier|ta|</w>
        </l>
      </lg>

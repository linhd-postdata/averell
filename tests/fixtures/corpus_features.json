[
    {
        "poem_title": "De La villana de la Sagra ",
        "manually_checked": false,
        "author": "Tirso de Molina",
        "stanzas": [
            {
                "stanza_number": "1",
                "stanza_type": "cuarteto",
                "lines": [
                    {
                        "line_number": "1",
                        "line_text": " Reino famoso, adiós, que alegre hago",
                        "metrical_pattern": "+--+-+-+-+-"
                    },
                    {
                        "line_number": "2",
                        "line_text": "ausencia de tu célebre montaña,",
                        "metrical_pattern": "-+---+---+-"
                    },
                    {
                        "line_number": "3",
                        "line_text": "pues que siendo mi patria, como extraña",
                        "metrical_pattern": "--+--+---+-"
                    },
                    {
                        "line_number": "4",
                        "line_text": "diste a mi juventud siempre mal pago.",
                        "metrical_pattern": "+----++-++-"
                    }
                ],
                "stanza_text": " Reino famoso, adiós, que alegre hago\nausencia de tu célebre montaña,\npues que siendo mi patria, como extraña\ndiste a mi juventud siempre mal pago."
            },
            {
                "stanza_number": "2",
                "stanza_type": "cuarteto",
                "lines": [
                    {
                        "line_number": "5",
                        "line_text": "Adiós ciudad, sepulcro de Santiago,",
                        "metrical_pattern": "-+-+-+---+-"
                    },
                    {
                        "line_number": "6",
                        "line_text": "que das pastor y das nobleza a España;",
                        "metrical_pattern": "-+-+-+-+-+-"
                    },
                    {
                        "line_number": "7",
                        "line_text": "adiós fin de la tierra, que el mar baña,",
                        "metrical_pattern": "-++--+--++-"
                    },
                    {
                        "line_number": "8",
                        "line_text": "reino famoso, del inglés estrago.",
                        "metrical_pattern": "+--+---+-+-"
                    }
                ],
                "stanza_text": "Adiós ciudad, sepulcro de Santiago,\nque das pastor y das nobleza a España;\nadiós fin de la tierra, que el mar baña,\nreino famoso, del inglés estrago."
            }
        ]
    },
    {
        "poem_title": "De La elección por la virtud ",
        "manually_checked": false,
        "author": "Tirso de Molina",
        "stanzas": [
            {
                "stanza_number": "1",
                "stanza_type": "cuarteto",
                "lines": [
                    {
                        "line_number": "1",
                        "line_text": "Pintadas aves que al pulir la aurora",
                        "metrical_pattern": "-+-+---+-+-"
                    },
                    {
                        "line_number": "2",
                        "line_text": "con peines de oro sus compuestas hebras,",
                        "metrical_pattern": "-+-+---+-+-"
                    },
                    {
                        "line_number": "3",
                        "line_text": "al son de arroyos, arpas de estas quiebras,",
                        "metrical_pattern": "-+-+-+-+-+-"
                    },
                    {
                        "line_number": "4",
                        "line_text": "lisonjeáis cada mañana a Flora.",
                        "metrical_pattern": "---++--+-+-"
                    }
                ],
                "stanza_text": "Pintadas aves que al pulir la aurora\ncon peines de oro sus compuestas hebras,\nal son de arroyos, arpas de estas quiebras,\nlisonjeáis cada mañana a Flora."
            }
        ]
    },
    {
        "poem_title": "Al capitán Domingo Alonso de la Maza, caballero del hábito de Santiago ",
        "manually_checked": false,
        "author": "Pantaleón de  Ribera,  Anastasio",
        "stanzas": [
            {
                "stanza_number": "1",
                "stanza_type": "cuarteto",
                "lines": [
                    {
                        "line_number": "1",
                        "line_text": "Esta de Santiago Espada ardiente",
                        "metrical_pattern": "+---+-+--+-"
                    },
                    {
                        "line_number": "2",
                        "line_text": "(merecido blasón, timbre glorioso)",
                        "metrical_pattern": "--+--++--+-"
                    },
                    {
                        "line_number": "3",
                        "line_text": "que ennoblece tu pecho belicoso,",
                        "metrical_pattern": "--+--+---+-"
                    },
                    {
                        "line_number": "4",
                        "line_text": "que solicita lauros a tu frente.",
                        "metrical_pattern": "---+-+---+-"
                    }
                ],
                "stanza_text": "Esta de Santiago Espada ardiente\n(merecido blasón, timbre glorioso)\nque ennoblece tu pecho belicoso,\nque solicita lauros a tu frente."
            },
            {
                "stanza_number": "2",
                "stanza_type": "cuarteto",
                "lines": [
                    {
                        "line_number": "5",
                        "line_text": "Sonoras trompas a tu fama aumente,",
                        "metrical_pattern": "-+-+---+-+-"
                    },
                    {
                        "line_number": "6",
                        "line_text": "pues con denuedo, y ánimo brioso,",
                        "metrical_pattern": "---+--+--+-"
                    },
                    {
                        "line_number": "7",
                        "line_text": "te ostentas Marte al bárbaro alevoso,",
                        "metrical_pattern": "-+-+-+---+-"
                    },
                    {
                        "line_number": "8",
                        "line_text": "te vibras rayo a la enemiga gente.",
                        "metrical_pattern": "-+-+---+-+-"
                    }
                ],
                "stanza_text": "Sonoras trompas a tu fama aumente,\npues con denuedo, y ánimo brioso,\nte ostentas Marte al bárbaro alevoso,\nte vibras rayo a la enemiga gente."
            },
            {
                "stanza_number": "3",
                "stanza_type": "terceto",
                "lines": [
                    {
                        "line_number": "9",
                        "line_text": "¿De dónde gloria tanta se ocasiona?",
                        "metrical_pattern": "-+-+-+---+-"
                    },
                    {
                        "line_number": "10",
                        "line_text": "Pues la primera cátedra presides",
                        "metrical_pattern": "---+-+---+-"
                    },
                    {
                        "line_number": "11",
                        "line_text": "de la fatal Escuela de Belona.",
                        "metrical_pattern": "---+-+---+-"
                    }
                ],
                "stanza_text": "¿De dónde gloria tanta se ocasiona?\nPues la primera cátedra presides\nde la fatal Escuela de Belona."
            },
            {
                "stanza_number": "4",
                "stanza_type": "terceto",
                "lines": [
                    {
                        "line_number": "12",
                        "line_text": "Como árbitro absoluto de sus lides,",
                        "metrical_pattern": "-+---+---+-"
                    },
                    {
                        "line_number": "13",
                        "line_text": "¿de dónde? de qué tu valor te abona",
                        "metrical_pattern": "-+--+--+-+-"
                    },
                    {
                        "line_number": "14",
                        "line_text": "por Maza fuerte del invicto Alcides.",
                        "metrical_pattern": "-+-+---+-+-"
                    }
                ],
                "stanza_text": "Como árbitro absoluto de sus lides,\n¿de dónde? de qué tu valor te abona\npor Maza fuerte del invicto Alcides."
            }
        ]
    }
]

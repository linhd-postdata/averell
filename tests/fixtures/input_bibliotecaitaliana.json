    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit001597",
        "author": "Iacopo Sannazaro",
        "collection": "Rime disperse",
        "title": "II",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Spargi di lauri, palme e mirti foglie,"
                },
                {
                    "verse": "viator, ché per qui intorno il spirto erra"
                },
                {
                    "verse": "di quel repente fólgore di guerra"
                },
                {
                    "verse": "che già de tutto el mondo adusse spoglie."
                }
            ],
            [
                {
                    "verse": "Questa urna angusta il ciner sacro accoglie,"
                },
                {
                    "verse": "ma el nome, qual in tomba non si serra,"
                },
                {
                    "verse": "tiene insepolto il ciel, l' acqua e la terra,"
                },
                {
                    "verse": "secur da tempo e da sue ingorde voglie."
                }
            ]
        ]
    },
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit000019",
        "author": "Dante Alighieri",
        "collection": "Commedia",
        "title": "I",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Nel mezzo del cammin di nostra vita"
                },
                {
                    "verse": "mi ritrovai per una selva oscura,"
                },
                {
                    "verse": "ché la diritta via era smarrita."
                }
            ],
            [
                {
                    "verse": "Ahi quanto a dir qual era è cosa dura"
                },
                {
                    "verse": "esta selva selvaggia e aspra e forte"
                },
                {
                    "verse": "che nel pensier rinova la paura!"
                }
            ],
            [
                {
                    "verse": "Allor si mosse, e io li tenni dietro."
                }
            ]
        ]
    }
]